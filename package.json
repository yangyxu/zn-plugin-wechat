{
  "name": "zn-plugin-wechat",
  "version": "0.2.2",
  "description": "前端基于zeanium-react-web框架, 后端基于zeanium-node框架的微信公众号模块，如需开发微信公众号，只需要在zn项目中安装改插件即可使用",
  "main": "index.js",
  "scripts": {
    "build": "babel ./web/src -d ./web/build",
    "dist": "webpack",
    "webpack.watch": "webpack --watch",
    "release": "npm run build & npm run dist",
    "install.release": "npm install --production",
    "install.dev": "npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yangyxu/zn-plugin-wechat.git"
  },
  "keywords": [
    "plugin",
    "zn",
    "zn-plugin",
    "wechat",
    "微信公众号"
  ],
  "author": "yangyxu",
  "license": "MIT",
  "devDependencies": {
    "zeanium-react-web": "^0.4.2"
  },
  "dependencies": {
    "md5": "^2.2.1",
    "qrcode.react": "^0.9.3",
    "request": "^2.88.0",
    "xml2js": "^0.4.19"
  }
}
